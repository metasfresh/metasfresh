	<parameter name="C_BPartner_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
	pl.name,
	validfrom::date,
	now()::date,
	bp.value
FROM
	M_Pricelist_Version plv
	INNER JOIN M_Pricelist pl ON plv.M_Pricelist_ID = pl.M_Pricelist_ID	AND pl.isActive = 'Y'
	INNER JOIN C_BPartner bp ON true	AND bp.isActive = 'Y'
WHERE
	M_Pricelist_Version_ID = $P{M_PriceList_Version_ID}
	AND bp.C_BPartner_ID = $P{C_BPartner_ID}
	AND plv.isActive = 'Y'
;]]>
	</queryString>
