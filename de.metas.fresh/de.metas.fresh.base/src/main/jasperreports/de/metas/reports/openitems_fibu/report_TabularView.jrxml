	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="IsSOTrx" class="java.lang.String" isForPrompting="false"/>
	<parameter name="Reference_Date" class="java.util.Date"/>
	<parameter name="AD_Org_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
	getDate() AS today,
	CASE
		WHEN $P{IsSOTrx} = 'Y' THEN 'Ja'
		WHEN $P{IsSOTrx} = 'N' THEN 'Nein'
		WHEN $P{IsSOTrx} IS NULL THEN 'Alle'
	END AS IsSOTrx,
	c.iso_code,
	oi.DocumentNo,
	oi.DateInvoiced::date,
	oi.NetDays,
	oi.DiscountDays,
	oi.DueDate::date,
	oi.DaysDue,
 	oi.DiscountDate::date,
	oi.GrandTotal,
	oi.PaidAmt,
	oi.OpenAmt,
	bp.Value,
	bp.Name,
	oi.PassForPayment,
	oi.faname,
	oi.fa_amount,
	oi.fa_source_amount,
	oi.ad_org_id
FROM
	(
	SELECT
		i.AD_Org_ID,
		i.DocumentNo,
		i.C_BPartner_ID,
		i.IsSOTrx,
		i.DateInvoiced,
		EXISTS (SELECT 0 FROM C_PaySelectionLine psl WHERE psl.processed = 'Y' AND psl.C_Invoice_ID = i.C_Invoice_ID AND psl.isActive = 'Y') AS PassForPayment,
		COALESCE ( p.NetDays, DaysBetween( ips.DueDate::timestamp with time zone, i.DateInvoiced::timestamp with time zone ) ) AS NetDays,
		p.DiscountDays,
		COALESCE( PaymentTermDueDate( p.C_PaymentTerm_ID, i.DateInvoiced::timestamp with time zone ), ips.DueDate ) AS DueDate,
		COALESCE(
			PaymentTermDueDays(i.C_PaymentTerm_ID, i.DateInvoiced::timestamp with time zone, $P{Reference_Date}),
			DaysBetween($P{Reference_Date}, ips.DueDate::timestamp with time zone)
		) AS DaysDue,
		COALESCE( AddDays( i.DateInvoiced::timestamp with time zone, p.DiscountDays ), ips.DiscountDate ) AS DiscountDate,
		COALESCE( Round( i.GrandTotal * p.Discount / 100::numeric, 2 ), ips.DiscountAmt ) AS DiscountAmt,
		COALESCE( ips.DueAmt, i.GrandTotal ) AS GrandTotal,
		COALESCE(
			invoicepaidtodate(i.C_Invoice_ID, i.C_Currency_ID, i.multiplierAP, $P{Reference_Date}),
			COALESCE( ips.DueAmt, i.GrandTotal ) - invoiceopentodate( i.C_Invoice_ID, COALESCE ( ips.C_InvoicePaySchedule_ID, 0::numeric ), $P{Reference_Date} )
		) AS PaidAmt,
		invoiceopentodate ( i.C_Invoice_ID, COALESCE ( ips.C_InvoicePaySchedule_ID, 0::numeric ), $P{Reference_Date} ) AS OpenAmt,
		i.InvoiceCollectionType,
		fa.C_Currency_ID, -- we use the currency from fact account
		ev.value || ' '|| ev.name as faname,
		(fa.AmtAcctCr - fa.AmtAcctDr) * acctBalance(fa.account_id, 0, 1 ) AS fa_amount, -- grand total of fact account
		(fa.AmtsourceCr - fa.AmtsourceDr) * acctBalance(fa.account_id, 0, 1 ) AS fa_source_amount -- grand total of invoice

	FROM
		fact_acct fa
		INNER JOIN C_Invoice_v i ON fa.Record_ID = i.C_Invoice_ID AND i.isActive = 'Y'
		LEFT OUTER JOIN C_PaymentTerm p ON i.C_PaymentTerm_ID = p.C_PaymentTerm_ID AND p.isActive = 'Y'
		LEFT OUTER JOIN C_InvoicePaySchedule ips ON i.C_Invoice_ID = ips.C_Invoice_ID AND ips.isvalid = 'Y' AND ips.isActive = 'Y'
		LEFT OUTER JOIN C_ElementValue ev on ev.C_ElementValue_ID = fa.Account_ID AND ev.isActive = 'Y'

		LEFT OUTER JOIN C_BP_Vendor_Acct va ON va.C_BPartner_ID = i.C_BPartner_ID AND va.isActive = 'Y'
		LEFT OUTER JOIN C_BP_Customer_Acct ca ON ca.C_BPartner_ID = i.C_BPartner_ID AND ca.isActive = 'Y'
		INNER JOIN C_ValidCombination vcom ON vcom.Account_id = fa.account_id AND CASE WHEN $P{IsSOTrx} = 'N' THEN vcom.C_ValidCombination_ID = va.V_Liability_Acct ELSE vcom.C_ValidCombination_ID = ca.C_Receivable_Acct END
											 AND vcom.isActive = 'Y'
											 
	WHERE
		invoiceopentodate( i.C_Invoice_ID, COALESCE( ips.C_InvoicePaySchedule_ID, 0::numeric ), $P{Reference_Date}  ) <> 0::numeric
		AND i.DocStatus IN ('CO','CL','RE')
		AND fa.AD_Table_ID = Get_Table_ID('C_Invoice')
		AND fa.isActive = 'Y'

	ORDER BY
		 i.DocumentNo
	)as oi
	INNER JOIN C_BPartner bp 	ON oi.C_BPartner_ID = bp.C_BPartner_ID
	INNER JOIN AD_Org o 		ON o.AD_Org_ID = oi.AD_Org_ID
	INNER JOIN AD_OrgInfo ori 	ON ori.AD_Org_ID = o.AD_Org_ID
	INNER JOIN C_Currency c 	ON oi.C_Currency_ID = c.C_Currency_ID

WHERE
	 oi.IsSOTrx = (CASE WHEN $P{IsSOTrx} IS NULL THEN oi.IsSOTrx ELSE $P{IsSOTrx} END)
	 AND oi.ad_org_id = $P{AD_Org_ID}

ORDER BY
	c.iso_code,
	bp.Name,
	bp.value,
	oi.DateInvoiced,
	oi.DocumentNo
;]]>
	</queryString>
