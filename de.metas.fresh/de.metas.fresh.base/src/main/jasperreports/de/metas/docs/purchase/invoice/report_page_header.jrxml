	<queryString>
		<![CDATA[SELECT
	DocumentNo,
	DateInvoiced,
	COALESCE( dtt.PrintName, dt.PrintName ) AS Printname
FROM
	C_Invoice i
	LEFT OUTER JOIN C_DocType dt ON i.C_DocTypeTarget_ID = dt.C_DocType_ID AND dt.isActive = 'Y'
	LEFT OUTER JOIN C_DocType_trl dtt ON dt.C_DocType_ID = dtt.C_DocType_ID AND dtt.isActive = 'Y'
		AND dtt.AD_Language = $P{ad_language}
WHERE
	i.C_Invoice_ID = $P{RECORD_ID} AND i.isActive = 'Y'
;]]>
	</queryString>
