	<queryString>
		<![CDATA[SELECT
	Name, --Product
	SUM(Incoming) AS Incoming,
	SUM(Outgoing) AS Outgoing,
	UOMSymbol
FROM
	de_metas_endcustomer_fresh_reports.Docs_HUBalance_Report_Details
		(
			540000::numeric,
			(SELECT C_BPartner_ID FROM M_InOut WHERE M_InOut_ID = $P{RECORD_ID}),
			null::date,
			now()::date,
			now()::date,
			$P{ad_language}
		) mbd
GROUP BY
	Name, --Product
	UOMSymbol
ORDER BY
	Name
;]]>
