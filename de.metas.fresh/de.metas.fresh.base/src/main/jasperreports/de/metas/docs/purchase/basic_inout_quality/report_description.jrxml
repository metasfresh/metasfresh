	<queryString>
		<![CDATA[SELECT
	io.DocumentNo,
	bp.value,
	io.movementDate,
	COALESCE (dtt.Printname, dt.Printname) AS Printname,
	dt.docbasetype,
	dt.docsubtype
FROM
	M_InOut io
	INNER JOIN C_DocType dt ON io.C_DocType_ID = dt.C_DocType_ID AND dt.isActive = 'Y'
	LEFT OUTER JOIN C_DocType_Trl dtt ON dt.C_DocType_ID = dtt.C_DocType_ID AND dtt.AD_Language = $P{ad_language} AND dtt.isActive = 'Y'
	INNER JOIN C_BPartner bp ON io.C_BPartner_ID = bp.C_BPartner_ID AND bp.isActive = 'Y'
WHERE
	M_InOut_ID = $P{RECORD_ID} AND io.isActive = 'Y'
;]]>
