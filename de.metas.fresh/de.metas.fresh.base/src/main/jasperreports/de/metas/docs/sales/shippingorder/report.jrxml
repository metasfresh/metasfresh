	<queryString>
		<![CDATA[SELECT
	st.AD_Org_ID,
	st.DocStatus,
	dt.PrintName,
	(
		SELECT	bpl.C_BPartner_Location_ID
		FROM	C_BPartner_Location bpl
		WHERE 	bp.C_BPartner_ID = bpl.C_BPartner_ID AND bpl.isActive = 'Y'
		ORDER BY 	isbilltodefault DESC, isbillto DESC
		LIMIT 1
	) AS C_BPartner_Location_ID
FROM
	M_ShipperTransportation st
	INNER JOIN M_Shipper s ON st.M_Shipper_ID = s.M_Shipper_ID AND s.isActive = 'Y'
	INNER JOIN C_BPartner bp ON s.C_BPartner_ID = bp.C_BPartner_ID AND bp.isActive = 'Y'
	INNER JOIN C_DocType dt ON st.C_DocType_ID = dt.C_DocType_ID AND dt.isActive = 'Y'
	LEFT OUTER JOIN C_DocType_Trl dtt ON st.C_DocType_ID = dtt.C_DocType_ID AND dtt.AD_Language = $P{ad_language} AND dtt.isActive = 'Y'
WHERE
	st.M_ShipperTransportation_ID = $P{RECORD_ID}
	AND st.isActive = 'Y'
;]]>
	</queryString>
