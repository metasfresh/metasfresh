	<queryString>
		<![CDATA[SELECT
	(
		SELECT DISTINCT Regexp_Replace(o.BPartnerAddress, E'\n', ', ', 'g')
		FROM	M_HU_Trx_Line hutl
			INNER JOIN M_ReceiptSchedule rs ON hutl.Record_ID = rs.M_ReceiptSchedule_ID
			INNER JOIN C_OrderLine ol ON rs.Record_ID = ol.C_OrderLine_ID
			INNER JOIN C_Order o ON ol.C_Order_ID = o.C_Order_ID
		WHERE	hutl.M_HU_ID = lu.M_HU_ID
			AND hutl.AD_Table_ID = (SELECT AD_Table_ID FROM AD_Table WHERE TableName = 'M_ReceiptSchedule')
			AND rs.AD_Table_ID = (SELECT AD_Table_ID FROM AD_Table WHERE TableName = 'C_OrderLine')
		LIMIT 1
	)  AS vendoraddress,
	bp.naics AS vendorgap,
	(
		SELECT	COALESCE(bp.name||', ', '') || Regexp_Replace(address, E'\n', ', ', 'g')
		FROM	C_BPartner bp
			INNER JOIN C_BPartner_Location bpl ON bp.C_BPartner_ID = bpl.C_BPartner_ID
		WHERE	bpl.C_BPartner_ID = lua_pra.valuenumber
	) AS produceraddress,
	lua_pra.naics AS producergap,
	lua_her.name AS Herkunft,
	lua_cha.value AS Charge,
	lua_lab.name AS Label,
	lua_anb.name AS Anbau,
	"de.metas.handlingunits".HU_LotNumberDate_ToString(lua_LotNumberDate.valueDate::date) AS LotNumberDate,
	(
		SELECT DISTINCT o.documentno
		FROM	M_HU_Trx_Line hutl
			INNER JOIN M_ReceiptSchedule rs ON hutl.Record_ID = rs.M_ReceiptSchedule_ID
			INNER JOIN C_OrderLine ol ON rs.Record_ID = ol.C_OrderLine_ID
			INNER JOIN C_Order o ON ol.C_Order_ID = o.C_Order_ID
		WHERE	hutl.M_HU_ID = lu.M_HU_ID
			AND hutl.AD_Table_ID = (SELECT AD_Table_ID FROM AD_Table WHERE TableName = 'M_ReceiptSchedule')
			AND rs.AD_Table_ID = (SELECT AD_Table_ID FROM AD_Table WHERE TableName = 'C_OrderLine')
		LIMIT 1
	) AS orderdocNo,
	p.name AS p_name,
	lua_adr.name AS adr,
	lu.created AS receiptdate,
	lua_beu.valuenumber AS Beurteilung,
	lua_not.name AS notiz,
	lua_add.name AS zusatz,
	lua_idn.lot AS tracking,
	lua_mhd.value AS mhd,
	lu.value AS receiptdocno,
	piip.name AS PackingInstruction,
	(
		SELECT DISTINCT io.movementdate
		FROM M_HU_Assignment huas
			INNER JOIN M_InOutLine iol ON huas.Record_ID = iol.M_InoutLine_ID AND huas.AD_Table_ID = Get_Table_ID('M_InOutLine')
			INNER JOIN M_InOut io on iol.M_InOut_ID = io.M_InOut_ID
		WHERE	huas.M_HU_ID = lu.M_HU_ID AND io.DocStatus in ('CO','CL')
		LIMIT 1
	) AS deliverydate
FROM
	M_HU lu
	INNER JOIN M_HU_Storage lus ON lu.M_HU_ID = lus.M_HU_ID
	INNER JOIN M_Product p ON lus.M_product_ID = p.M_Product_ID
	LEFT OUTER JOIN C_BPartner bp ON lu.C_BPartner_ID = bp.C_BPartner_ID
	LEFT OUTER JOIN "de.metas.handlingunits".M_ToplevelHU_HU_PI_Item_Product piip ON lu.M_HU_ID = piip.M_HU_ID AND piip.M_Product_ID = p.M_Product_ID
	LEFT OUTER JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_lua.valuenumber, sub_bp.naics
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN C_BPartner sub_bp ON sub_lua.ValueNumber = sub_bp.C_BPartner_ID
		WHERE 	sub_a.value = 'SubProducerBPartner'
	) lua_pra ON lu.M_HU_ID = lua_pra.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = '1000001' --Herkunft
	) lua_her ON lu.M_HU_ID = lua_her.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_lua.value
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
		WHERE 	sub_a.value = 'Lot-Nummer'
	) lua_cha ON lu.M_HU_ID = lua_cha.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = '1000002' -- Label
	) lua_lab ON lu.M_HU_ID = lua_lab.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = '1000004' -- Produktionsart
	) lua_anb ON lu.M_HU_ID = lua_anb.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = '1000015' -- AdR
			AND sub_av.value != '01' -- don't display 'Keine / Leer'
	) lua_adr ON lu.M_HU_ID = lua_adr.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_lua.valuenumber
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
		WHERE 	sub_a.value = 'QualityDiscountPercent'
	) lua_beu ON lu.M_HU_ID = lua_beu.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = 'QualityNotice'
	) lua_not ON lu.M_HU_ID = lua_not.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_av.name
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_AttributeValue sub_av ON sub_lua.value = sub_av.value AND sub_a.M_Attribute_ID = sub_av.M_Attribute_ID
		WHERE 	sub_a.value = '1000020' -- Zitrusfr체chte
	) lua_add ON lu.M_HU_ID = lua_add.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_mt.lot
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
			INNER JOIN M_Material_Tracking sub_mt ON sub_lua.value = sub_mt.M_Material_Tracking_ID::character varying
		WHERE 	sub_a.value = 'M_Material_Tracking_ID'
	) lua_idn ON lu.M_HU_ID = lua_idn.M_HU_ID
	LEFT JOIN (
		SELECT 	sub_lua.M_HU_ID, sub_lua.value
		FROM 	M_HU_Attribute sub_lua
			INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
		WHERE 	sub_a.value = '1000021' --MHD
	) lua_mhd ON lu.M_HU_ID = lua_mhd.M_HU_ID
	LEFT JOIN (
	SELECT 	sub_lua.M_HU_ID, sub_lua.valueDate
	FROM 	M_HU_Attribute sub_lua
		INNER JOIN M_Attribute sub_a ON sub_lua.M_Attribute_ID = sub_a.M_Attribute_ID
		WHERE sub_a.value = 'HU_LotNumberDate'
	)lua_LotNumberDate ON lu.M_HU_ID = lua_LotNumberDate.M_HU_ID

WHERE
	lu.M_HU_ID = $P{M_HU_ID}
;]]>
	</queryString>
