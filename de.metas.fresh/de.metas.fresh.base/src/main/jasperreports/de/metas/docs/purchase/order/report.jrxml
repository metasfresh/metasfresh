	<queryString>
		<![CDATA[SELECT
	o.AD_Org_ID,
	o.DocStatus,
	dt.PrintName,
	CASE
		WHEN
		EXISTS(
			SELECT 0
			FROM C_OrderLine ol
			INNER JOIN M_Product p ON ol.M_Product_ID = p.M_Product_ID
			INNER JOIN M_Product_Category pc ON p.M_Product_Category_ID = pc.M_Product_Category_ID
			WHERE pc.M_Product_Category_ID = (SELECT value::numeric FROM AD_SysConfig WHERE name = 'PackingMaterialProductCategoryID')
				AND ol.C_Order_ID = o.C_Order_ID
		)
		THEN 'Y'
		ELSE 'N'
	END as displayhu
FROM
	C_Order o
	INNER JOIN C_DocType dt ON o.C_DocType_ID = dt.C_DocType_ID
	LEFT OUTER JOIN C_DocType_Trl dtt ON o.C_DocType_ID = dtt.C_DocType_ID AND dtt.AD_Language = $P{ad_language}
WHERE
	o.C_Order_ID = $P{RECORD_ID}
;]]>
	</queryString>
