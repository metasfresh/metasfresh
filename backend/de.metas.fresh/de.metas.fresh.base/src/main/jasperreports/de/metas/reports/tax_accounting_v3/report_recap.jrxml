	<queryString>
		<![CDATA[SELECT TaxAmt_SUM,
       TaxAmt_SUM_PrevYear,
       currency,
       source_currency,
       (CASE
            WHEN Currency = source_currency THEN
                (NetAmt_SUM + TaxAmt_SUM)
                                            ELSE NULL
        END)                     AS TotalAmt_SUM,
       NetAmt_SUM,
       Taxname,
       vatcode
FROM de_metas_acct.report_taxaccounts(p_AD_Org_ID =>  $P{AD_Org_ID},
                                      p_account_id => NULL,
                                      p_c_vat_code_id => NULL,
                                      p_datefrom => $P{DateFrom},
                                      p_dateto => $P{DateTo},
                                      p_level => 'ReCap'
                                      )
  ORDER BY vatcode;]]>
	</queryString>
