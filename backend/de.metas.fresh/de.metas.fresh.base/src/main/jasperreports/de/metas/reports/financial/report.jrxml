	<queryString>
		<![CDATA[select
	o.name as org_name,
	o.description as org_description,
	oi.taxid as org_taxid,
	oi.duns as org_duns,
	l.address1 as org_address1,
	l.address2 as org_address2,
	l.address3 as org_address3,
	l.address4 as org_address4,
	l.postal as org_postal,
	l.postal_add as org_postal_add,
	l.city as org_city,
	c.name as org_country_name,

	-- metas: adressaufbereitung organisation
	trim(
		case when l.address1 is null then '' else l.address1 end || ' ' ||
		case when l.address2 is null then '' else l.address2 end || ' ' ||
		case when l.address3 is null then '' else l.address3 end || ' ' ||
		case when l.address4 is null then '' else l.address4 end
	) as org_address_row,
	trim (
		case when l.postal_add is null then '' else l.postal_add end || ' ' ||
		case when l.city is null then '' else l.city end
	) as org_postal_row,
	(SELECT ad_org_parm.name FROM ad_org ad_org_parm WHERE ad_org_parm.ad_org_id in (null, $P{Org_ID}) and ad_org_parm.isActive = 'Y') AS parm_org_name,
	(SELECT pr.name FROM pa_report pr WHERE pi.record_id = pr.pa_report_id AND pr.isActive = 'Y') AS report_name,
	(SELECT per.name FROM c_period per WHERE  per.c_period_id in (null, $P{C_Period_ID}) AND per.isActive = 'Y' LIMIT 1) AS parm_period_name,
	(SELECT bp.name FROM c_bpartner bp WHERE bp.c_bpartner_id in (null, $P{C_BPartner_ID}) AND bp.isActive = 'Y' LIMIT 1) AS parm_bpartner_name,
	(SELECT pro.name FROM m_product pro WHERE pro.m_product_id in (null, $P{M_Product_ID}) AND pro.isActive = 'Y' LIMIT 1) AS parm_product_name,
	(SELECT hier.name FROM pa_hierarchy hier WHERE hier.pa_hierarchy_id in (null, $P{PA_Hierarchy_ID}) AND hier.isActive = 'Y' LIMIT 1) AS parm_hierarchy_name
FROM
	ad_org o
	LEFT OUTER JOIN AD_OrgInfo oi ON o.AD_Org_ID = oi.AD_Org_ID AND oi.isActive = 'Y' 
	LEFT OUTER JOIN C_BPartner bp ON o.AD_Org_ID = bp.AD_OrgBP_ID AND bp.isActive = 'Y' 
	LEFT OUTER JOIN C_BPartner_Location bpl ON bp.C_BPartner_ID = bpl.C_BPartner_ID AND bpl.isActive = 'Y' 
	LEFT OUTER JOIN C_Location l ON bpl.C_Location_ID = l.C_Location_ID AND l.isActive = 'Y' 
	LEFT OUTER JOIN C_Country c ON l.C_Country_ID = c.C_Country_ID AND c.isActive = 'Y' 
	LEFT OUTER JOIN AD_PInstance pi ON pi.ad_pinstance_id = $P{T_Report_AD_PInstance_ID} AND pi.isActive = 'Y' 
	LEFT OUTER JOIN pa_report pr ON pi.record_id = pr.pa_report_id AND pr.isActive = 'Y' 
WHERE
	o.ad_org_id = $P{ad_org_id} AND o.isActive = 'Y' 
ORDER BY
	isBillToDefault desc,
	isBillto desc
LIMIT
	1
;]]>
	</queryString>
