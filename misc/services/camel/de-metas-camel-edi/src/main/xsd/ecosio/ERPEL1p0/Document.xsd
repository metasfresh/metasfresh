				<xs:annotation>
					<xs:documentation>Bank identification code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="BankAccountNr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bank account number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="IBAN" minOccurs="0">
				<xs:annotation>
					<xs:documentation>International Bank Account Number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="BankAccountOwner" minOccurs="0">
				<xs:annotation>
