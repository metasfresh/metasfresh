			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalLineItemInformationType">
		<xs:sequence>
			<!-- Free-text description of the item -->
			<xs:element ref="DrawingRevisionNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Revision number of the underlying drawing of the product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DrawingRevisionDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the underlying drawing of the product has been change</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="EngineeringChangeNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A control number indicating the engineering change</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ControlNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A control number which is assigned to the forecasted product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductionNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A production number to which the forecasted product is assigned to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModelYear" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>And indicator for the model year to which the forecasted product belongs to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BuyersInternalProductGroupCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An internal code of the buyer, identifying the product group to which the forecasted product belongs to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentificationOfUse" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies what the requested product is used for - e.g. pilot, additional request, specimem, etc. Contains a coded value. Has been introduced due to a requirements from the VDA 4905 document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialConditions" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator showing whether this is an article with special conditions, which is requested here. If no BLANK is provided here, the article is an article with special coniditions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgressNumberDifference" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Difference between the continuous entry number and the continuous demand number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KanbanID" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A set of kanban IDs, issued by the customer. These Kanban IDs can be taken over to the dispatch advice as well.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Classification" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The classification of the product/service in free-text form.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalInformationType">
		<xs:sequence>
			<xs:element ref="SerialNumber" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The serial number of the list line item product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ChargeNumber" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The number of the batch/lot.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Classification" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The classification of the product/service in free-text form.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="AlternativeQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to specify an alternative quantity in addition to ListLineItem/Quantity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="QuantityCharged" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to describe a quantity for which the customer is actually charged. May be provided in case this number deviates from ListLinteItem/Quantity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="QuantityFreeOfCharge" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to describe a quantity which is provided to the customer free-of-charge. May be provided in case this number deviates from ListLinteItem/Quantity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ChargeNumberExternal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An external batch/lot number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ChargeExpirationDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The expiration date is the last day in which a product is safe to consume.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="BestBeforeDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The best before date is the last day where the product is still in perfect condition.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ChargeDateOfManufacture" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The manufacturing date of a certain batch/lot.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="EngineeringChangeNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A control number/level indicating the engineering change.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="DrawingRevisionNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Revision number of the underlying drawing of the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="DeductionQuantity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						In case a deduction took place, the deducted quantity is provided here.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="StockLocation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The stock location where the goods item was taken from.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="StockGround" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The stock ground the goods item was taken from.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="DestinationStockLocation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The destination stock location where the goods item will be shipped to.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="DestinationStockGround" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The destination stock ground where the goods item will be shipped to.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Size" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The size of the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Weight" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The weight of the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Boxes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of boxes where the products of the given list line item are stored in.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Color" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The color of the product of the given list line item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryOfOrigin" type="CountryType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Country in which the product was produced or could be produced.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConfirmedCountryOfOrigin" type="CountryType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The confirmed country of origin, provided by the supplier. Even if a product can potentially be produced in multiple countries, it has only one confirmed country of origin.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element ref="Salutation" minOccurs="0">
