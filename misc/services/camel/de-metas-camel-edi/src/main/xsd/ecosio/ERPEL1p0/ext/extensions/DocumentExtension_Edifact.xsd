														<xs:simpleContent>
															<xs:extension base="xs:dateTime">
																<xs:attribute name="DateFunctionCodeQualifier" type="xs:string" use="optional"/>
																<xs:attribute name="DateFormatCode" type="xs:string" use="optional"/>
