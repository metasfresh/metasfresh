	<queryString>
		<![CDATA[SELECT

	CASE WHEN o.name IS NULL
		THEN ''
		ELSE o.name
	END AS orgname,
	CASE WHEN l.address1 IS NULL
		THEN ''
		ELSE l.address1
	END AS address1,
	CASE WHEN l.city IS NULL
		THEN ''
		ELSE l.city
	END AS city,
	CASE WHEN l.postal IS NULL
		THEN ''
		ELSE l.postal || ' '
	END AS postal
FROM
	ad_org o
	INNER JOIN C_BPArtner bp	   	ON o.ad_org_id = bp.AD_OrgBP_ID AND bp.isActive = 'Y'
	INNER JOIN C_BPartner_Location bpl 	ON bp.C_BPartner_ID = bpl.C_BPartner_ID AND bpl.isActive = 'Y'
	INNER JOIN C_location l 	   	ON bpl.c_location_id = l.c_location_id AND l.isActive = 'Y'
WHERE
	o.ad_org_id = 1000000 AND o.isActive = 'Y'
ORDER BY
	IsBillTo DESC,
	IsBillToDefault DESC,
	IsShipTo DESC,
	IsShipToDefault DESC
;]]>
	</queryString>
