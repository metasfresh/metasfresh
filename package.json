    "postcss-color-function": "~2.0.1",
    "postcss-import": "~8.1.2",
    "postcss-loader": "~0.9.1",
    "postcss-simple-vars": "~2.0.0",
    "postcss-url": "~5.1.2",
    "precss": "~1.4.0",
    "prettier": "^1.7.4",
    "react-hot-loader": "~3.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "~3.4.0",
